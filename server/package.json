{
  "name": "ts-battleship-server",
  "version": "1.0.0",
  "description": "The backend for ts-battleship",
  "main": "dist/index.js",
  "scripts": {
    "start": "concurrently npm:start:build npm:start:deploy",
		"start:build": "tsc -b",
		"start:run": "node ./dist/index.js",
		"start:deploy": "node ./server/dist/index.js",
    "dev": "concurrently npm:dev:*",
    "dev:build": "tsc -w",
    "dev:run": "nodemon ./dist/index.js",
    "lint": "npx prettier --config ./prettier.config.js ./src* --check",
    "lint:fix": "npx prettier --config ./prettier.config.js ./src* --write",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drewcook/ts-battleship.git"
  },
  "keywords": [
    "typescript",
    "battleship"
  ],
  "author": "Drew Cook",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/drewcook/ts-battleship/issues"
  },
  "homepage": "https://github.com/drewcook/ts-battleship#readme",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "concurrently": "^6.2.0",
    "http-status": "^1.5.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "nodemon": "^2.0.12"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa__cors": "^3.0.3",
    "@types/koa-bodyparser": "^4.3.4",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-mount": "^4.0.1",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "altheajs-prettier-config": "^1.1.4",
    "battleship-types": "^1.0.15",
    "prettier": "2.3.2"
  }
}
